{
  "expo": {
    "name": "Alicorn",
    "slug": "alicorn-test",
    "description": "University app for work projects and entrepreneurship relationships",
    "version": "1.0.0",
    "icon": "./assets/alicorn_logo.png",
    "splash": {
      "image": "./assets/alicorn_download.png",
      "resizeMode": "contain"
    },
    "scheme": "acme",
    "web": {
      "bundler": "metro",
      "userInterfaceStyle": "automatic"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.tobiascasazza.alicorn",
      "infoPlist": {
        "NSMotionUsageDescription": "Needs Permission for Steps"
      }
    },
    "android": {
      "googleServicesFile": "google-services.json",
      "userInterfaceStyle": "automatic",
      "package": "com.tobiascasazza.alicorn"
    },
    "plugins": [
      "./node_modules/expo-sample-pedometer/expo-sample-pedometer-plugin.js",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ad2b7524-466e-4698-aa03-34627ff4c9b3"
      }
    }
  }
}
